<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <property name="prefix" value="db_book"/>
    <property name="author" value="Slava"/>

    <changeSet id="v_1.0_id_10.1_module_book-service_connect_book\union_date_02022023.xml" author="${author}">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="db_book_book"/>
            <tableExists tableName="db_book_union"/>
        </preConditions>
    <!--откуда, что, куда, к чему    -->
        <addForeignKeyConstraint baseTableName="db_book_book"
                             baseColumnNames="id"
                             constraintName="fk_db_book_book_id_db_book_union_book"
                             referencedTableName="db_book_union"
                             referencedColumnNames="book"/>
        <rollback>
            <dropForeignKeyConstraint baseTableName="db_book_book"
                                  constraintName="fk_db_book_book_id_db_book_union_book"/>

        </rollback>
    </changeSet>
</databaseChangeLog>
